const [allData, setAllData] =  useState(null);  
        useEffect(()  =>{
             fetch('http://localhost:9000/users/lista')
                .then(res => res.json())
                .catch(error=> console.log(error))
                .then(res =>{ setAllData(res); })
                
    
        }, []); // los corchetes al final evitan un bucle infinito
    
        console.log(allData)



